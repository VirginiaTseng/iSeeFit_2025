# Qwen API Configuration
# Copy this file to .env and fill in your actual API key

# Required: Your Qwen API key from Alibaba Cloud
QWEN_API_KEY=your_qwen_api_key_here

# Optional: Custom API base URL (default: https://dashscope.aliyuncs.com/api/v1)
QWEN_BASE_URL=https://dashscope.aliyuncs.com/api/v1

# Optional: Model selection preferences
DEFAULT_MODEL=qwen-turbo
FALLBACK_MODEL=qwen-plus

# Optional: API timeout settings (in seconds)
API_TIMEOUT=30
MAX_RETRIES=3
